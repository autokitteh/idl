    "/api/v1/litterboxes": {
      "post": {
        "operationId": "LitterBox_Setup",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/litterboxSetupResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/litterboxSetupRequest"
            }
          }
        ],
        "tags": [
          "LitterBox"
        ]
      }
    },
    "/api/v1/litterboxes/{id}/run": {
      "post": {
        "operationId": "LitterBox_Run",
        "responses": {
          "200": {
            "description": "A successful response.(streaming responses)",
            "schema": {
              "type": "object",
              "properties": {
                "result": {
                  "$ref": "#/definitions/litterboxRunUpdate"
                },
                "error": {
                  "$ref": "#/definitions/rpcStatus"
                }
              },
              "title": "Stream result of litterboxRunUpdate"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "id",
            "description": "If id is empty, run will setup, use and scoop the litterbox by itself.",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "source": {
                  "type": "string"
                },
                "event": {
                  "$ref": "#/definitions/litterboxSyntheticEvent"
                }
              }
            }
          }
        ],
        "tags": [
          "LitterBox"
        ]
      }
    },
